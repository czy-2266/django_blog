{"ast":null,"code":"import { Star, InfoFilled, Tools, DataBoard, Grid, Switch, Connection, Service, Coin, Lock, Files, Edit, FolderOpened, Search, Picture, User, Monitor, Share, Link, ChatDotRound, Message } from '@element-plus/icons-vue';\nexport default {\n  name: 'AboutPage',\n  components: {\n    Star,\n    InfoFilled,\n    Tools,\n    DataBoard,\n    Grid,\n    Switch,\n    Connection,\n    Service,\n    Coin,\n    Lock,\n    Files,\n    Edit,\n    FolderOpened,\n    Search,\n    Picture,\n    User,\n    Monitor,\n    Share,\n    Link,\n    ChatDotRound,\n    Message\n  },\n  methods: {\n    openGithub() {\n      window.open('https://github.com/ice-blog/ice-blog', '_blank');\n    }\n  }\n};","map":{"version":3,"names":["Star","InfoFilled","Tools","DataBoard","Grid","Switch","Connection","Service","Coin","Lock","Files","Edit","FolderOpened","Search","Picture","User","Monitor","Share","Link","ChatDotRound","Message","name","components","methods","openGithub","window","open"],"sources":["/Users/a123/PythonProject/System/django_blog/frontend/src/components/AboutPage.vue"],"sourcesContent":["<template>\n  <div class=\"about-page\">\n    <div class=\"hero-section\">\n      <div class=\"hero-content\">\n        <h1>\n          <el-icon><Star /></el-icon>\n          关于 Ice BLOG\n        </h1>\n        <p class=\"hero-subtitle\">一个优雅的个人博客系统</p>\n      </div>\n    </div>\n\n    <div class=\"content-sections\">\n      <!-- 项目介绍 -->\n      <section class=\"intro-section\">\n        <h2>\n          <el-icon><InfoFilled /></el-icon>\n          项目介绍\n        </h2>\n        <div class=\"intro-content\">\n          <p>\n            Ice BLOG 是一个基于现代Web技术栈构建的个人博客系统，致力于提供简洁、优雅、高效的写作和阅读体验。\n          </p>\n          <p>\n            系统采用前后端分离的架构设计，前端使用 Vue 3 + Element Plus，后端采用 FastAPI + SQLAlchemy，\n            支持文章发布、分类管理、全文搜索、用户认证等核心功能。\n          </p>\n        </div>\n      </section>\n\n      <!-- 技术栈 -->\n      <section class=\"tech-section\">\n        <h2>\n          <el-icon><Tools /></el-icon>\n          技术栈\n        </h2>\n        <div class=\"tech-stacks\">\n          <div class=\"tech-category\">\n            <h3>前端技术</h3>\n            <div class=\"tech-items\">\n              <div class=\"tech-item\">\n                <el-icon><DataBoard /></el-icon>\n                <div>\n                  <h4>Vue 3</h4>\n                  <p>渐进式JavaScript框架</p>\n                </div>\n              </div>\n              <div class=\"tech-item\">\n                <el-icon><Grid /></el-icon>\n                <div>\n                  <h4>Element Plus</h4>\n                  <p>Vue 3 组件库</p>\n                </div>\n              </div>\n              <div class=\"tech-item\">\n                <el-icon><Switch /></el-icon>\n                <div>\n                  <h4>Pinia</h4>\n                  <p>Vue 状态管理</p>\n                </div>\n              </div>\n              <div class=\"tech-item\">\n                <el-icon><Connection /></el-icon>\n                <div>\n                  <h4>Vue Router</h4>\n                  <p>前端路由管理</p>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <div class=\"tech-category\">\n            <h3>后端技术</h3>\n            <div class=\"tech-items\">\n              <div class=\"tech-item\">\n                <el-icon><Service /></el-icon>\n                <div>\n                  <h4>Django</h4>\n                  <p>现代化Python Web框架</p>\n                </div>\n              </div>\n              <div class=\"tech-item\">\n                <el-icon><Coin /></el-icon>\n                <div>\n                  <h4>SQLAlchemy</h4>\n                  <p>Python SQL工具包和ORM</p>\n                </div>\n              </div>\n              <div class=\"tech-item\">\n                <el-icon><Lock /></el-icon>\n                <div>\n                  <h4>JWT</h4>\n                  <p>JSON Web Token认证</p>\n                </div>\n              </div>\n              <div class=\"tech-item\">\n                <el-icon><Files /></el-icon>\n                <div>\n                  <h4>MySQL</h4>\n                  <p>关系型数据库</p>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </section>\n\n      <!-- 功能特性 -->\n      <section class=\"features-section\">\n        <h2>\n          <el-icon><Star /></el-icon>\n          功能特性\n        </h2>\n        <div class=\"features-grid\">\n          <div class=\"feature-card\">\n            <div class=\"feature-icon\">\n              <el-icon><Edit /></el-icon>\n            </div>\n            <h3>文章管理</h3>\n            <p>支持Markdown语法，富文本编辑，草稿保存，定时发布</p>\n          </div>\n          \n          <div class=\"feature-card\">\n            <div class=\"feature-icon\">\n              <el-icon><FolderOpened /></el-icon>\n            </div>\n            <h3>分类管理</h3>\n            <p>灵活的分类体系，支持多级分类，便于内容组织</p>\n          </div>\n          \n          <div class=\"feature-card\">\n            <div class=\"feature-icon\">\n              <el-icon><Search /></el-icon>\n            </div>\n            <h3>全文搜索</h3>\n            <p>强大的搜索功能，支持标题、内容、标签全文检索</p>\n          </div>\n          \n          <div class=\"feature-card\">\n            <div class=\"feature-icon\">\n              <el-icon><Picture /></el-icon>\n            </div>\n            <h3>文件上传</h3>\n            <p>支持图片、文档等多种文件格式上传和管理</p>\n          </div>\n          \n          <div class=\"feature-card\">\n            <div class=\"feature-icon\">\n              <el-icon><User /></el-icon>\n            </div>\n            <h3>用户系统</h3>\n            <p>完整的用户认证体系，支持注册、登录、权限管理</p>\n          </div>\n          \n          <div class=\"feature-card\">\n            <div class=\"feature-icon\">\n              <el-icon><Monitor /></el-icon>\n            </div>\n            <h3>响应式设计</h3>\n            <p>完美适配桌面端和移动端，提供一致的用户体验</p>\n          </div>\n        </div>\n      </section>\n\n      <!-- 开源信息 -->\n      <section class=\"opensource-section\">\n        <h2>\n          <el-icon><Share /></el-icon>\n          开源协议\n        </h2>\n        <div class=\"opensource-content\">\n          <p>\n            Ice BLOG 遵循 MIT 开源协议，欢迎大家使用、修改和分发。\n          </p>\n          <div class=\"github-info\">\n            <el-card class=\"github-card\">\n              <div class=\"repo-info\">\n                <h3>GitHub 仓库</h3>\n                <p>欢迎 Star、Fork 和贡献代码</p>\n                <el-button type=\"primary\" @click=\"openGithub\">\n                  <el-icon><Link /></el-icon>\n                  访问 GitHub\n                </el-button>\n              </div>\n            </el-card>\n          </div>\n        </div>\n      </section>\n\n      <!-- 联系方式 -->\n      <section class=\"contact-section\">\n        <h2>\n          <el-icon><ChatDotRound /></el-icon>\n          联系方式\n        </h2>\n        <div class=\"contact-content\">\n          <p>如果您有任何问题、建议或合作意向，欢迎通过以下方式联系我们：</p>\n          <div class=\"contact-items\">\n            <div class=\"contact-item\">\n              <el-icon><Message /></el-icon>\n              <span>邮箱：contact@iceblog.dev</span>\n            </div>\n            <div class=\"contact-item\">\n              <el-icon><ChatDotRound /></el-icon>\n              <span>QQ群：123456789</span>\n            </div>\n          </div>\n        </div>\n      </section>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {\n  Star,\n  InfoFilled,\n  Tools,\n  DataBoard,\n  Grid,\n  Switch,\n  Connection,\n  Service,\n  Coin,\n  Lock,\n  Files,\n  Edit,\n  FolderOpened,\n  Search,\n  Picture,\n  User,\n  Monitor,\n  Share,\n  Link,\n  ChatDotRound,\n  Message\n} from '@element-plus/icons-vue'\n\nexport default {\n  name: 'AboutPage',\n  components: {\n    Star,\n    InfoFilled,\n    Tools,\n    DataBoard,\n    Grid,\n    Switch,\n    Connection,\n    Service,\n    Coin,\n    Lock,\n    Files,\n    Edit,\n    FolderOpened,\n    Search,\n    Picture,\n    User,\n    Monitor,\n    Share,\n    Link,\n    ChatDotRound,\n    Message\n  },\n  \n  methods: {\n    openGithub() {\n      window.open('https://github.com/ice-blog/ice-blog', '_blank')\n    }\n  }\n}\n</script>\n\n<style scoped>\n.about-page {\n  max-width: 1200px;\n  margin: 0 auto;\n}\n\n.hero-section {\n  background: linear-gradient(135deg, #87CEEB 0%, #4682B4 100%);\n  border-radius: 16px;\n  padding: 80px 40px;\n  text-align: center;\n  margin-bottom: 40px;\n  color: white;\n}\n\n.hero-content h1 {\n  font-size: 48px;\n  margin: 0 0 16px 0;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  gap: 20px;\n}\n\n.hero-subtitle {\n  font-size: 18px;\n  margin: 0;\n  opacity: 0.9;\n}\n\n.content-sections {\n  display: flex;\n  flex-direction: column;\n  gap: 40px;\n}\n\nsection {\n  background: white;\n  border-radius: 12px;\n  padding: 32px;\n  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);\n}\n\nsection h2 {\n  display: flex;\n  align-items: center;\n  gap: 12px;\n  color: #2c3e50;\n  margin: 0 0 24px 0;\n  font-size: 24px;\n}\n\n.intro-content p {\n  line-height: 1.8;\n  color: #555;\n  margin-bottom: 16px;\n}\n\n.tech-stacks {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\n  gap: 32px;\n}\n\n.tech-category h3 {\n  color: #2c3e50;\n  margin-bottom: 20px;\n  font-size: 18px;\n}\n\n.tech-items {\n  display: flex;\n  flex-direction: column;\n  gap: 16px;\n}\n\n.tech-item {\n  display: flex;\n  align-items: center;\n  gap: 16px;\n  padding: 16px;\n  border: 1px solid #f0f0f0;\n  border-radius: 8px;\n  transition: all 0.3s ease;\n}\n\n.tech-item:hover {\n  border-color: #87CEEB;\n  background: #f8fcff;\n}\n\n.tech-item .el-icon {\n  font-size: 24px;\n  color: #87CEEB;\n}\n\n.tech-item h4 {\n  margin: 0 0 4px 0;\n  color: #2c3e50;\n}\n\n.tech-item p {\n  margin: 0;\n  color: #718096;\n  font-size: 14px;\n}\n\n.features-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\n  gap: 24px;\n}\n\n.feature-card {\n  text-align: center;\n  padding: 24px;\n  border: 1px solid #f0f0f0;\n  border-radius: 12px;\n  transition: all 0.3s ease;\n}\n\n.feature-card:hover {\n  border-color: #87CEEB;\n  transform: translateY(-4px);\n  box-shadow: 0 8px 25px rgba(135, 206, 235, 0.2);\n}\n\n.feature-icon {\n  width: 64px;\n  height: 64px;\n  border-radius: 50%;\n  background: linear-gradient(135deg, #87CEEB, #4682B4);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  margin: 0 auto 16px;\n}\n\n.feature-icon .el-icon {\n  font-size: 28px;\n  color: white;\n}\n\n.feature-card h3 {\n  color: #2c3e50;\n  margin: 0 0 12px 0;\n}\n\n.feature-card p {\n  color: #718096;\n  line-height: 1.6;\n  margin: 0;\n}\n\n.opensource-content p {\n  line-height: 1.8;\n  color: #555;\n  margin-bottom: 24px;\n}\n\n.github-info {\n  display: flex;\n  justify-content: center;\n}\n\n.github-card {\n  max-width: 400px;\n  text-align: center;\n}\n\n.repo-info h3 {\n  margin: 0 0 12px 0;\n  color: #2c3e50;\n}\n\n.repo-info p {\n  color: #718096;\n  margin: 0 0 20px 0;\n}\n\n.contact-content p {\n  line-height: 1.8;\n  color: #555;\n  margin-bottom: 24px;\n}\n\n.contact-items {\n  display: flex;\n  flex-direction: column;\n  gap: 16px;\n}\n\n.contact-item {\n  display: flex;\n  align-items: center;\n  gap: 12px;\n  padding: 16px;\n  background: #f8f9fa;\n  border-radius: 8px;\n}\n\n.contact-item .el-icon {\n  color: #87CEEB;\n  font-size: 18px;\n}\n\n@media (max-width: 768px) {\n  .about-page {\n    margin: 12px;\n  }\n  \n  .hero-section {\n    padding: 60px 24px;\n  }\n  \n  .hero-content h1 {\n    font-size: 32px;\n    flex-direction: column;\n    gap: 12px;\n  }\n  \n  .hero-subtitle {\n    font-size: 16px;\n  }\n  \n  section {\n    padding: 24px;\n  }\n  \n  section h2 {\n    font-size: 20px;\n  }\n  \n  .tech-stacks {\n    grid-template-columns: 1fr;\n    gap: 24px;\n  }\n  \n  .features-grid {\n    grid-template-columns: 1fr;\n    gap: 16px;\n  }\n  \n  .contact-items {\n    gap: 12p